{
  "name": "birch-lounge-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "rebuild-frontend": "node scripts/automated-frontend-rebuild.js && npm run build && npm run lint:fix",
    "dev": "vite --port 3000",
    "dev:debug": "vite --port 3000 --debug",
    "dev:host": "vite --port 3000 --host",
    "build": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "lint:check": "eslint . --ext js,jsx,ts,tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:debug": "vitest --reporter=verbose",
    "test:staged": "vitest related --run",
    "test:related": "vitest related",
    "test:a11y": "vitest --run src/test/**/*.a11y.test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "security:audit": "node scripts/security-audit.cjs",
    "security:check": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "security:report": "npm run security:audit && npm run security:check",
    "precommit": "npm run security:audit && npm run lint && npm run test",
    "storybook:build": "storybook build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:incremental": "tsc --noEmit --incremental",
    "debug:chrome": "start chrome --remote-debugging-port=9222 --user-data-dir=./chrome-debug-profile",
    "debug:edge": "start msedge --remote-debugging-port=9222 --user-data-dir=./edge-debug-profile",
    "perf:analyze": "npm run build && npx vite-bundle-analyzer",
    "perf:visualize": "npm run build && npx rollup-plugin-visualizer",
    "perf:lighthouse": "npm run build && npx lighthouse-ci autorun",
    "perf:source-map": "npm run build && npx source-map-explorer 'dist/assets/*.js'",
    "a11y:test": "npm run build && npx axe-core",
    "a11y:audit": "npm run build && npx lighthouse-ci autorun --config=.lighthouserc.json",
    "quality:check": "npm run lint:check && npm run format:check && npm run type-check && npm run test:coverage",
    "quality:fix": "npm run lint:fix && npm run format",
    "prepare": "husky",
    "clean": "rm -rf dist storybook-static coverage .nyc_output",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "clean:all": "npm run clean && npm run clean:deps",
    "mcp:install": "node scripts/install-mcp.js",
    "mcp:test": "node scripts/test-mcp.js",
    "mcp:status": "node scripts/mcp-status.js",
    "mcp:start": "node scripts/start-mcp.js",
    "mcp:stop": "node scripts/stop-mcp.js",
    "mcp:restart": "npm run mcp:stop && npm run mcp:start",
    "mcp:logs": "node scripts/mcp-logs.js",
    "mcp:health": "node scripts/mcp-health.js",
    "dev-mcp:install": "node scripts/install-dev-mcps.js",
    "dev-mcp:test": "node scripts/test-dev-mcps.js",
    "dev-mcp:analyze": "node scripts/mcp-code-analysis.js",
    "dev-mcp:test-auto": "node scripts/mcp-test-automation.js",
    "dev-mcp:performance": "node scripts/mcp-performance.js",
    "dev-mcp:docs": "node scripts/mcp-documentation.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "@octokit/rest": "^22.0.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "notion-client": "^7.6.0",
    "openai": "^5.23.0",
    "pdfjs-dist": "^5.4.149",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.1.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "path-browserify": "^1.0.1",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "process": "^0.11.10",
    "prop-types": "15.8.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "util": "^0.12.5",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  }
}
